<script src="https://js.sentry-cdn.com/1fcecc392aa0480cb106746fe7fb50e4.min.js" crossorigin="anonymous"></script>
<script>
    Sentry.onLoad(function() {
        Sentry.init({
        integrations: [
            Sentry.replayIntegration(),
        ],
        release: "website@<%= del.version %>",
        // Session Replay
        replaysSessionSampleRate: 0.1,
        replaysOnErrorSampleRate: 1.0,
        });
    });
</script>
<style>body{background-color:black;color:white;}</style>
<h1 style="color: red;"><%= __("page.error.critical") %></h1>
<h2><%= message %></h2>
<h3><%= error.status %></h3>
<pre><%= error.stack %></pre>
<br />
<button onclick="window.location = '<% if (typeof(linkPrefix) !== "undefined") { %><%= linkPrefix %><% } else { %>en-US<% } %>/';"><%= __("common.home") %></button>
<button onclick="history.back();"><%= __("common.goback") %></button>
<p id="premidPageInfo" style="display: none;"><%= __("premid.error.critical") %></p>